{
    "source" : {
        "include" : ["./server.js", "src/components"],
        "includePattern" : ".+\\.(vue|js)$",
        "excludePattern" : "(^|\\/|\\\\)_"
    },
    "opts" : {
        "destination" : "./docs",
        "recurse" : true,
        "readme" : "./README.md"
    },
    "plugins": ["./node_modules/jsdoc-vuejs"]
}